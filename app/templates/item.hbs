<h2>{{model.name}}</h2>
<p>Price: ${{model.price}}</p>
<p>Description: {{model.description}}</p>
<p>Quantity: {{model.quantity}}</p>
<img src={{model.image}}>
<br>

<button {{action 'addToCart' model}} class="btn btn-success">Add To Cart</button>

{{display-cart checkout='checkout' refresh="refresh"}}
